             var PageURL = this.PageURL;
             var url = window.location.href;
            //
            PageURL = url;

             var data ={
               Text: updatedText,
               PageURL : PageURL,
               ElementId : ElementName
             }

             sessionStorage.setItem('data', JSON.stringify(data));


             console.log(data);

            //  $.get('/updateAboutUs', function(data){
